{
  "name": "slackr-cli",
  "version": "0.0.2",
  "description": "CLI interface for slack interactions",
  "main": "./bin/slackr",
  "bin": {
    "slackr": "./bin/slackr"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --pretty --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "slackr:status": "./bin/slackr status",
    "slackr:standup": "./bin/slackr standup"
  },
  "keywords": [
    "slackr",
    "slack",
    "cli",
    "nodejs",
    "typescript",
    "yargs"
  ],
  "author": "Jordan Garrison (https://github.com/jordangarrison)",
  "repository": {
    "type": "git",
    "url": "https://github.com/jordangarrison/slackr.git"
  },
  "bugs": {
    "url": "https://github.com/jordangarrison/slackr/issues"
  },
  "homepage": "https://github.com/jordangarrison/slackr",
  "license": "MIT",
  "dependencies": {
    "@slack/web-api": "^6.1.0",
    "dotenv": "^8.2.0",
    "enquirer": "^2.3.6",
    "esm": "^3.2.25",
    "humanize-ms": "^1.2.1",
    "lodash": "^4.17.21",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/humanize-ms": "^1.2.0",
    "@types/lodash": "^4.14.168",
    "@types/yargs": "^15.0.12",
    "typescript": "^4.1.3"
  },
  "files": [
    "dist/**",
    "bin/**",
    "README.md"
  ],
  "engines": {
    "node": "~14"
  },
  "release": {
    "branches": [
      {
        "name": "main"
      }
    ]
  }
}
